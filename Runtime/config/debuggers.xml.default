       </SerializedObject>
       <SerializedObject Name="DebuggerCmd1">
         <wxString Value="wxString" Name="m_name"/>
         <wxString Value="($(Variable)).m_impl._M_dataplus._M_p" Name="m_command"/>
         <wxString Value="print" Name="m_dbgCommand"/>
       </SerializedObject>
     </SerializedObject>
  </ArchiveObject>
</Debuggers>
