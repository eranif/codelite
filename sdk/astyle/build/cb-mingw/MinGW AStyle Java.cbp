<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="MinGW AStyle Java" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/AStyle301jd.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="objJava/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-std=c++11" />
					<Add option="-fno-rtti" />
					<Add option="-fno-exceptions" />
					<Add option="-DASTYLE_JNI" />
					<Add directory="$(JAVA_HOME)/include" />
					<Add directory="$(JAVA_HOME)/include/win32" />
				</Compiler>
				<Linker>
					<Add option="-Wl,--add-stdcall-alias" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/AStyle301j.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="objJava/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c++11" />
					<Add option="-fno-rtti" />
					<Add option="-fno-exceptions" />
					<Add option="-DNDEBUG" />
					<Add option="-DASTYLE_JNI" />
					<Add directory="$(JAVA_HOME)/include" />
					<Add directory="$(JAVA_HOME)/include/win32" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-Wl,--add-stdcall-alias" />
				</Linker>
			</Target>
			<Target title="Lint">
				<Option output="bin/AStyle301jl.dll" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="0" extension_auto="0" />
				<Option object_output="objJava/Lint/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Wnon-virtual-dtor" />
					<Add option="-Wshadow" />
					<Add option="-Winit-self" />
					<Add option="-Wredundant-decls" />
					<Add option="-Wcast-align" />
					<Add option="-Wundef" />
					<Add option="-Wfloat-equal" />
					<Add option="-Winline" />
					<Add option="-Wunreachable-code" />
					<Add option="-Wmissing-declarations" />
					<Add option="-Wmissing-include-dirs" />
					<Add option="-Wswitch-enum" />
					<Add option="-Wmain" />
					<Add option="-pedantic" />
					<Add option="-std=c++0x" />
					<Add option="-std=c++98" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-Wint-to-pointer-cast" />
					<Add option="-Woverloaded-virtual" />
					<Add option="-Wwrite-strings" />
					<Add option="-std=c++11" />
					<Add option="-fno-exceptions" />
					<Add option="-fno-rtti" />
					<Add option="-DASTYLE_JNI" />
					<Add directory="$(JAVA_HOME)/include" />
					<Add directory="$(JAVA_HOME)/include/win32" />
				</Compiler>
			</Target>
		</Build>
		<Unit filename="../../src/ASBeautifier.cpp" />
		<Unit filename="../../src/ASEnhancer.cpp" />
		<Unit filename="../../src/ASFormatter.cpp" />
		<Unit filename="../../src/ASResource.cpp" />
		<Unit filename="../../src/astyle.h" />
		<Unit filename="../../src/astyle_main.cpp" />
		<Unit filename="../../src/astyle_main.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
